   "source": [
    "# Nhập giá trị độ Celsius từ người dùng\n",
    "celsius = float(input(\"Nhập giá trị độ Celsius: \"))\n",
    "\n",
    "# Tính toán độ Fahrenheit\n",
    "fahrenheit = (celsius * 1.8) + 32\n",
    "\n",
    "# Hiển thị kết quả\n",
    "print(f\"{celsius} độ Celsius tương ứng với {fahrenheit} độ Fahrenheit.\")\n"
   ]
  }
